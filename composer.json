{
    "name": "alexgoncharcherkassy/chain-command-bundle",
    "description": "Symfony bundle - implements command chaining functionality.",
    "type": "symfony-bundle",
    "homepage": "https://github.com/alexgoncharcherkassy/ChainCommandBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Honchar",
            "email": "cs210785gaa@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AlexGoncharCK\\ChainCommandBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexGoncharCK\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "require": {
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/yaml": "5.2.*",
        "symfony/monolog-bridge": "^5.2"
    }
}
